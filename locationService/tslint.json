{
    "rules": {
      "no-floating-promises": true,
      "array-type": [true, "array"],
      "switch-default": true,
      "adjacent-overload-signatures": true,
      "class-name": true,
      "curly": true,
      "cyclomatic-complexity": [
        true,
        15
      ],
      "indent": [
        true,
        "spaces"
      ],
      "max-file-line-count": [
        true,
        500
      ],
      "max-line-length": [
        true,
        120
      ],
      "member-access": true,
      "member-ordering": [
        true,
        {
          "order": "fields-first"
        }
      ],
      "no-default-export": true,
      "no-duplicate-variable": true,
      "no-eval": true,
      "no-internal-module": true,
      "no-parameter-properties": true,
      "no-trailing-whitespace": false,
      "no-unsafe-finally": true,
      "no-var-keyword": true,
      "one-line": [
        true,
        "check-open-brace",
        "check-whitespace"
      ],
      "prefer-const": true,
      "quotemark": [
        true,
        "single"
      ],
      "semicolon": [
        true,
        "always"
      ],
      "space-before-function-paren": [
        true,
        "never"
      ],
      "typedef-whitespace": [
        true,
        {
          "call-signature": "nospace",
          "index-signature": "nospace",
          "parameter": "nospace",
          "property-declaration": "nospace",
          "variable-declaration": "nospace"
        }
      ],
      "no-unused-expression": true,
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-trailing-underscore",
        "allow-pascal-case",
        "allow-leading-underscore"
      ],
      "whitespace": [
        true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-separator",
        "check-type"
      ]
    }
  }
